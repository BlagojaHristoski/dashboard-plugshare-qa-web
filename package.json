{
  "name": "dashboard-plugshare-web",
  "private": true,
  "version": "0.0.1",
  "scripts": {
    "clean:jenkins": "del /q jenkinsresults",
    "clean:windows": "del /q output",
    "clean": "rm -rf ./output",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "lint:staged": "lint-staged",
    "results": "allure serve output -p 8088",
    "spell-check": "cspell",
    "test": "codeceptjs run --verbose --grep",
    "test:all": "codeceptjs run --verbose ",
    "test:tagged": "./run-tagged-tests.sh",
    "test:home": "codeceptjs run ./tests/home/home.test.ts --verbose --override",
    "test:locations": "codeceptjs run ./tests/locations/locations.test.ts --verbose --override",
    "test:locations:entries": "codeceptjs run ./tests/locations/locations.entries.test.ts --verbose --override",
    "test:locations:details": "codeceptjs run ./tests/locations/locations.details.test.ts --verbose --override",
    "test:searchandfilters": "codeceptjs run ./tests/locations/searchandfilters.test.ts --verbose --override",
    "test:activityfilters": "codeceptjs run ./tests/activity/activity.filters.test.ts --verbose --override",
    "test:activitylocations": "codeceptjs run ./tests/activity/activity.locations.test.ts --verbose --override",
    "test:activitysettings": "codeceptjs run ./tests/activity/activity.settings.test.ts --verbose --override",
    "test:footer": "codeceptjs run ./tests/footer.test.ts --verbose --override",
    "testjenkins": "codeceptjs run --verbose --reporter mocha-junit-reporter --grep",
    "testjenkins:all": "codeceptjs run --verbose --reporter mocha-junit-reporter",
    "testjenkins:home": "codeceptjs run ./tests/home/home.test.ts --verbose --override --reporter mocha-junit-reporter",
    "testjenkins:locations": "codeceptjs run ./tests/locations/locations.test.ts --verbose --override --reporter mocha-junit-reporter",
    "testjenkins:locations:entries": "codeceptjs run ./tests/locations/locations.entries.test.ts --verbose --override --reporter mocha-junit-reporter",
    "testjenkins:locations:details": "codeceptjs run ./tests/locations/locations.details.test.ts --verbose --override --reporter mocha-junit-reporter",
    "testjenkins:searchandfilters": "codeceptjs run ./tests/locations/searchandfilters.test.ts --verbose --override --reporter mocha-junit-reporter",
    "testjenkins:activityfilters": "codeceptjs run ./tests/activity/activity.filters.test.ts --verbose --override --reporter mocha-junit-reporter",
    "testjenkins:activitylocations": "codeceptjs run ./tests/activity/activity.locations.test.ts --verbose --override --reporter mocha-junit-reporter",
    "testjenkins:activitysettings": "codeceptjs run ./tests/activity/activity.settings.test.ts --verbose --override --reporter mocha-junit-reporter",
    "testjenkins:footer": "codeceptjs run ./tests/footer.test.ts --verbose --override --reporter mocha-junit-reporter"
  },
  "dependencies": {
    "@codeceptjs/mailslurp-helper": "^1.0.7",
    "codeceptjs": "^3.4.1",
    "codeceptjs-chai": "^2.3.5",
    "cspell": "^6.31.1",
    "dotenv": "^16.0.3",
    "eslint": "^8.37.0",
    "husky": "^8.0.3",
    "mailslurp": "^0.0.1-security",
    "mocha-junit-reporter": "^2.2.0",
    "mochawesome": "^7.1.3"
  },
  "devDependencies": {
    "@types/node": "^18.14.6",
    "mocha": "^10.2.0",
    "playwright": "1.18",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  }
}
